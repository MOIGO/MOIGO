		<typeAlias type="com.kh.moigo.admin.model.vo.MemberDetail" alias="memberDetail"/>	
		<typeAlias type="com.kh.moigo.admin.model.vo.GroupDetail" alias="groupDetail"/>					
		<typeAlias type="com.kh.moigo.admin.model.vo.MemPerGroup" alias="memPerGroup"/>		
		<typeAlias type="com.kh.moigo.admin.model.vo.GrpMember" alias="grpMember"/>		
