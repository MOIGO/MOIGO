		<typeAlias type="com.kh.moigo.groups.model.vo.Groups" alias="groups"/>
		<typeAlias type="com.kh.moigo.groups.model.vo.GroupMember" alias="groupMember"/>
		<typeAlias type="com.kh.moigo.member.model.vo.Member" alias="member"/>

		<typeAlias type="com.kh.moigo.groups.model.vo.Post" alias="post"/>
		<typeAlias type="com.kh.moigo.groups.model.vo.Schedule" alias="schedule"/>
